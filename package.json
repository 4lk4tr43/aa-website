		"eslint-plugin-svelte3": "^4.0.0",
		"npm-check-updates": "^16.7.10",
		"prettier": "^2.8.4",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.4",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"type": "module"
}
